<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="SpeakFriend.Utilities.StoredFile, SpeakFriend.Utilities"
      table="StoredFile" lazy="false">

    <id name="Id" column="Id" type="Int32">
      <generator class="native" />
    </id>

    <property name="EntityType" column="EntityType" type="string" not-null="true"/>
    <property name="EntityId" column="EntityId" type="int" not-null="true"/>
    <property name="OriginalFileName" column="OriginalFileName" type="string" not-null="true"/>


    <property name="DateCreated" column="DateCreated" type="DateTime" not-null="true"/>

  </class>
</hibernate-mapping>